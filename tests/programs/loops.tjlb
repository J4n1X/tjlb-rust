# Test loops
fn main() -> i32 {
    i32 sum = 0

    # While loop: sum 1..5
    i32 i = 1
    while i <= 5 {
        sum = sum + i
        i = i + 1
    }

    # For loop: multiply by 2
    for (i32 j = 0; j < 2; j = j + 1) {
        sum = sum * 2
    }

    return sum  # Should be (1+2+3+4+5) * 2 * 2 = 15 * 4 = 60
}
